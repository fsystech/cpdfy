{
  "author": "Safe Online World Ltd.",
  "name": "html-pdf-c-plus-plus",
  "version": "0.0.1",
  "description": "HTML string to PDF converter with wkhtmltopdf",
  "main": "index.js",
  "type": "commonjs",
  "types": "index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/safeonlineworld/html-pdf-c-plus-plus.git"
  },
  "scripts": {
    "install": "node-pre-gyp install --build-from-source",
    "test": "node test.js",
    "app": "node64 test.js",
    "build:dev": "node-gyp build --debug",
    "build": "node-gyp clean configure build --verbose --arch=ia32",
    "rebuild:dev": "node-gyp rebuild --debug",
    "rebuild": "node-gyp rebuild",
    "clean": "node-gyp clean"
  },
  "binary": {
    "module_name": "html_pdf_c",
    "module_path": "./lib/binding/{configuration}/{node_abi}-{platform}-{arch}/"
  },
  "license": "ISC",
  "devDependencies": {},
  "bundledDependencies":["node-pre-gyp"],
  "dependencies": {
    "nan": "^2.14.2",
    "node-pre-gyp": "^0.17.0"
  }
}
